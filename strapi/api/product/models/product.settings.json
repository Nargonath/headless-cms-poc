{
  "connection": "default",
  "collectionName": "Products",
  "info": {
    "name": "Product"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "category": {
      "via": "products",
      "model": "category"
    }
  }
}
